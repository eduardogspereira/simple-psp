{
  "extends": ["airbnb-base", "plugin:jest/recommended", "prettier"],
  "plugins": ["prettier", "jest", "no-async-without-await"],
  "rules": {
    "no-console": "error",
    "no-async-without-await/no-async-without-await": "error",
    "prettier/prettier": "error",
    "complexity": [
      "error",
      {
        "max": 3
      }
    ]
  }
}
